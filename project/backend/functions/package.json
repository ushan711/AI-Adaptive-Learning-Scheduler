{
  "name": "ai-learning-scheduler-functions",
  "version": "1.0.0",
  "description": "Firebase Cloud Functions for AI Adaptive Learning Scheduler",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.20.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "typescript": "^5.1.6"
  }
}